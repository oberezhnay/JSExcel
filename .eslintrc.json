{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "arrow-parens": "off",
        "linebreak-style": 0,
        "comma-dangle": "off",
        "require-jsdoc": "off"
    },
    "extends": ["eslint:recommended", "google"]
}