{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest/globals": true
    },
    "rules": {
        "semi": "off",
        "arrow-parens": "off",
        "linebreak-style": 0,
        "comma-dangle": "off",
        "require-jsdoc": "off",
        "operator-linebreak": "off"
    },
    "extends": ["eslint:recommended", "google"]
}